<!DOCTYPE html>
<html lang="en">

<head>
  {% include "basehead.njk" %}
  {% block head %}{% endblock %}
</head>

<body>
<div class="container">
<p style="text-align:center;margin:0;padding:0;">
  <a class="skip-to-nav-link" href="#navigation">
                skip to navigation
            </a>
            <a class="skip-to-content-link" href="#maincont">
        skip to content
      </a>
  </p>
            <button onclick="topFunction()" id="top" title="Go to top">Back to Top</button>
  <div class="header">
      <header>
        {% include 'header.njk' %}
      </header>
  </div>
  <div class="maincont">
      <div class="navigation">
       <nav>{% include 'navigation.njk' %}</nav>
    </div>

    <div class=content>
       <main>
       <a id="maincont"></a>
       {{ content | safe }}
       </main>
    </div>

    <div class="sidebar">
      <aside>{% include 'sidebar.njk' %}</aside>
    </div>

  </div>
  <div class="footer">
    <footer>{% include 'footer.njk' %}</footer>
  </div>
</div>
</body>
<script src="https://unpkg.com/@popperjs/core@2"></script>
<script src="https://unpkg.com/tippy.js@6"></script>
<script>
  tippy('[data-tippy-content]', {
    theme: 'faerie',
    animation: 'scale',
    touch: 'hold',
    inertia: true,
    arrow: true,
  });
</script>
<script src="../../../js/scrobbles.js{{ nocache }}"></script>
<script src="../../../js/updated-visitors.js{{ nocache }}"></script>
<script src="../../../js/backtotop.js{{ nocache }}"></script>
<script src="../../../js/tabs.js{{ nocache }}"></script>
<script src="../../../js/playgifs.js{{ nocache }}"></script>
<script src="../../../js/marquee.js{{ nocache }}"></script>